#!/bin/bash
# Latest GoShimmer version
currentGoshimmerVersion="$(curl -s -X GET "http://localhost:8080/info" -H "accept: application/json"|jq '.version')"
latestGoshimmer="$(curl -s https://api.github.com/repos/iotaledger/goshimmer/releases/latest | grep -oP '"tag_name": "\K(.*)(?=")')"
latestGoshimmer="${latestGoshimmer:1}"
