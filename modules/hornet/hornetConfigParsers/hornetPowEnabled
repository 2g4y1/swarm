#!/bin/bash

parser="$(jq '.restAPI.powEnabled' $hornetHome/$hornetNetwork.json)"
if [ "$hornetPowEnabled" = "true" ] || [ "$hornetPowEnabled" = "false" ]; then
    if [ "$parser" != "$hornetPowEnabled" ]; then
        sudo jq '.restAPI.powEnabled = '$hornetPowEnabled'' $hornetHome/$hornetNetwork.json|sponge $hornetHome/$hornetNetwork.json
        restartHornet=true
    fi
fi