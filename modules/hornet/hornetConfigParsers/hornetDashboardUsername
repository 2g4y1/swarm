#!/bin/bash
if [ ! -z "$hornetUsername" ]; then
    parser="$(jq '.dashboard.auth.username' $hornetHome/$hornetNetwork.json)"
    parser=$(echo $parser | tr -d '"')
    if [ "$hornetUsername" != "$parser" ]; then
        sudo jq '.dashboard.auth.username = "'$hornetUsername'"' $hornetHome/$hornetNetwork.json|sponge $hornetHome/$hornetNetwork.json
        restartHornet=true
        newHornetUsername=true
    fi
fi