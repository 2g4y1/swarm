#!/bin/bash
if [ ! -z "$hornetPasswordHash" ]; then
    parser="$(jq '.dashboard.auth.passwordHash' $hornetHome/$hornetNetwork.json)"
    parser=$(echo $parser | tr -d '"')
    if [ "$hornetPasswordHash" != "$parser" ]; then
        sudo jq '.dashboard.auth.passwordHash = "'$hornetPasswordHash'"' $hornetHome/$hornetNetwork.json|sponge $hornetHome/$hornetNetwork.json
    fi
fi