#!/bin/bash

IFS=', ' read -r -a ARRAYhornetConfigs <<< "$hornetConfigs"

for hornetConfig in "${ARRAYhornetConfigs[@]}"
    do
    parser="$(jq '.db.engine' $hornetDir/$hornetConfig.json)"
    if [ "$parser" != "\"$hornetDbEngine\"" ]; then
        sudo jq '.db.engine = "'$hornetDbEngine'"' $hornetDir/$hornetConfig.json|sponge $hornetDir/$hornetConfig.json
        restartHornet=true
        newHornetDbEngine=true
    fi
done
unset ARRAYhornetConfigs
unset hornetConfig