#!/bin/bash
if [ "$pruning" = "true" ]; then
    IFS=', ' read -r -a ARRAYhornetConfigs <<< "$hornetConfigs"

    for hornetConfig in "${ARRAYhornetConfigs[@]}"
    do
        parser="$(jq '.pruning.size.cooldownTime' $hornetDir/$hornetConfig.json)"
        if [ "$parser" != "$pruningCooldownTime" ]; then
            sudo jq '.pruning.size.cooldownTime = "'$pruningCooldownTime'"' $hornetDir/$hornetConfig.json|sponge $hornetDir/$hornetConfig.json
        fi
    done
    unset ARRAYhornetConfigs
    unset hornetConfig
fi