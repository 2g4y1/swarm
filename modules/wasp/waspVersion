#!/bin/bash
# Latest WASP version
waspVersion=$(curl -s -X GET "http://localhost:9090/info" -H "accept: application/json" |jq '.Version' | tr -d 'v' | tr -d '"')

latestWaspVersion=$(curl -s -X GET "https://cdn.tanglebay.com/github/version" -H  "accept: application/json"|jq '.data.node.wasp.stable' | tr -d '"')

# Replace var if empty
if [ -z "$latestWaspVersion" ]; then
    latestWaspVersion=$waspVersion
fi

if [ -z "$waspVersion" ]; then
    waspVersion=$latestWaspVersion
fi
