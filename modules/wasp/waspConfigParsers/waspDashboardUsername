#!/bin/bash

parser="$(jq '.dashboard.auth.username' $waspHome/config.json)"
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$waspDashboardUsername" ] && [ ! -z "$waspDashboardUsername" ] 2>/dev/null; then
    sudo jq '.dashboard.auth.username = "'$waspDashboardUsername'"' $waspHome/config.json|sponge $waspHome/config.json
    restartGoshimmer=true
fi