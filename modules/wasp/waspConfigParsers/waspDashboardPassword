#!/bin/bash

parser="$(jq '.dashboard.auth.password' $waspHome/config.json)"
parser=$(echo $parser | tr -d '"')
if [ "$parser" != "$waspDashboardPassword" ] && [ ! -z "$waspDashboardPassword" ] 2>/dev/null; then
    sudo jq '.dashboard.auth.username = "'$waspDashboardPassword'"' $waspHome/config.json|sponge $waspHome/config.json
    restartGoshimmer=true
fi